{
  "name": "@earthling/react-native-es",
  "version": "0.0.44",
  "description": "ES themed components",
  "source": "src/index.tsx",
  "main": "lib/module/index.js",
  "module": "lib/module/index.js",
  "react-native": "src/index.tsx",
  "types": "lib/typescript/index.d.ts",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "typings"
  ],
  "private": false,
  "scripts": {
    "build": "rm -rf ./lib/* && tsc",
    "prepare": "rm -rf ./lib/* && bob build",
    "cp": "rm -rf ./lib/* && bob build && cp -R ./lib ./examples/node_modules/@earthling/react-native-es && cp -R ./src ./examples/node_modules/@earthling/react-native-es",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/earthlingstd/react-native-es.git"
  },
  "author": "Earthling Studio",
  "license": "ISC",
  "devDependencies": {
    "@types/color": "^3.0.1",
    "@types/jest": "^26.0.16",
    "@types/react": "^17.0.0",
    "@types/react-native": "^0.63.37",
    "@types/react-native-vector-icons": "^6.4.6",
    "@types/react-test-renderer": "^17.0.0",
    "react": "^17.0.1",
    "react-native-builder-bob": "^0.17.1",
    "react-native-fast-image": "^8.3.4",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-vector-icons": "^7.1.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "color": "^3.1.3",
    "deepmerge": "^4.2.2",
    "hoist-non-react-statics": "^3.3.2"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "module",
      "typescript"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}
